      <input type="text" id="search" placeholder="Search for..." onchange="inputOnChange(this.value)">
      <button onclick="searchOnClick()">Search…</button>


  <div id="videoLink"></div>


<script>
var searchString = "";
function inputOnChange(val) {
    searchString = val;
    console.log(val);
}

function searchOnClick() {
    searchString = "https://www.youtube.com/results?search_query=" + searchString;
    console.log("Search button is clicked: " + searchString);
    window.open(searchString);
}
</script>
